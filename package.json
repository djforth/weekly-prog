{
  "name": "@djforth/weekly-prog",
  "version": "5.0.4",
  "description": "Better Weekly Prog",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "morsedigital",
    "rules": {
      "react/jsx-uses-vars": 1
    }
  },
  "assets": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "images": {
      "ext": [
        "*.png",
        "*.gif",
        "*.jpg",
        "*.jpeg",
        "*.svg"
      ],
      "plugins": [
        [
          "imagemin-gifsicle",
          {
            "interlaced": true
          }
        ],
        [
          "imagemin-jpegtran",
          {
            "progressive": true
          }
        ],
        [
          "imagemin-optipng",
          {
            "optimizationLevel": 3
          }
        ],
        "imagemin-svgo"
      ]
    },
    "javascripts": {
      "ext": [
        ".js",
        ".es6.js"
      ],
      "files": [
        "application.js"
      ],
      "ignore": [],
      "externals": [],
      "separate": true,
      "input": "test_assets/javascripts",
      "output": "app/assets/javascripts",
      "common": "common.js",
      "transforms": [
        [
          "babelify",
          {
            "presets": [
              "es2015",
              "react"
            ]
          }
        ]
      ]
    },
    "karma": {
      "input": "./spec"
    },
    "port": 9090,
    "rails_port": 3030,
    "uiport": 8080
  },
  "config": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "port": 4000,
    "rails_port": 3030,
    "uiport": 9080,
    "img_config": {
      "ext": [
        "*.png",
        "*.gif",
        "*.jpg",
        "*.jpeg",
        "*.svg"
      ]
    }
  },
  "scripts": {
    "browserSync": "node ./fe_build_scripts/browserSync.js",
    "build": "babel src -x '.js' -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "js": "asset-pipeline-browserify -x",
    "js:minify": "asset-pipeline-browserify -m -x",
    "js:watch": "asset-pipeline-browserify -w -x",
    "lint": "eslint src/**",
    "sass:build": "node ./fe_build_scripts/sass-build.js -m",
    "sass:watch": "node ./fe_build_scripts/sass-build.js -w",
    "start": "npm run browserSync & npm run build:watch",
    "test": "asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/weekly-prog.git"
  },
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/weekly-prog/issues"
  },
  "homepage": "https://github.com/djforth/weekly-prog#readme",
  "devDependencies": {
    "@djforth/ap_browserify": "^1.4.0",
    "@djforth/ap_imagemin": "2.0.7",
    "@djforth/ap_karma": "^1.1.10",
    "@djforth/ap_sprites": "1.0.10",
    "@djforth/ap_utils": "^2.0.13",
    "@djforth/morse-jasmine": "^1.0.16",
    "ajax-es6-module": "1.0.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.2",
    "browserify": "^13.0.0",
    "chokidar": "^1.4.3",
    "classnames": "^2.2.0",
    "datamanager": "1.0.2",
    "eslint": "^2.8.0",
    "eslint-config-google": "^0.5.0",
    "eslint-config-morsedigital": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "event-stream": "^3.3.2",
    "flux": "^2.1.1",
    "image-size": "^0.5.0",
    "immutable": "^3.7.5",
    "lodash": "~4.8.2",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "moment": "~2.12.0",
    "moment-strftime": "^0.1.5",
    "morse-react-mixins": "~1.2.0",
    "node-sass": "^3.4.2",
    "node-sass-globbing": "0.0.23",
    "path": "^0.12.7",
    "react": "~15.0.0",
    "react-addons-test-utils": "~15.0.0",
    "react-date-range": "~0.2.4",
    "react-dom": "~15.0.0",
    "react-jasmine": "~1.1.0",
    "readdirp": "^2.0.0",
    "sinon": "^1.17.3",
    "through2": "^2.0.1",
    "viewport-detection-es6": "2.0.4"
  },
  "peerDependencies": {
    "ajax-es6-module": "1.0.2",
    "datamanager": "1.0.2",
    "morse-react-mixins": "~1.2.0",
    "viewport-detection-es6": "2.0.4",
    "classnames": "^2.2.0",
    "flux": "^2.1.1",
    "immutable": "^3.7.5",
    "lodash": "~4.8.2",
    "moment": "~2.12.0",
    "moment-strftime": "^0.1.5",
    "react": "~15.0.0",
    "react-date-range": "~0.2.4",
    "react-dom": "~15.0.0"
  },
  "dependencies": {}
}
