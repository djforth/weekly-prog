{
  "name": "@djforth/weekly-prog",
  "version": "3.1.1",
  "description": "Better Weekly Prog",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "assets": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "images": {
      "ext": [
        "*.png",
        "*.gif",
        "*.jpg",
        "*.jpeg",
        "*.svg"
      ],
      "plugins": [
        [
          "imagemin-gifsicle",
          {
            "interlaced": true
          }
        ],
        [
          "imagemin-jpegtran",
          {
            "progressive": true
          }
        ],
        [
          "imagemin-optipng",
          {
            "optimizationLevel": 3
          }
        ],
        "imagemin-svgo"
      ]
    },
    "javascripts": {
      "ext": [
        ".js",
        ".es6.js"
      ],
      "files": [
        "application.js",
        "now.js"
      ],
      "ignore": [],
      "externals": [],
      "separate": true,
      "input": "test_assets/javascripts",
      "output": "app/assets/javascripts",
      "common": "common.js",
      "transforms": [
        [
          "babelify",
          {
            "presets": [
              "es2015",
              "react"
            ]
          }
        ]
      ]
    },
    "port": 9090,
    "rails_port": 3030,
    "uiport": 8080
  },
  "config": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "port": 4000,
    "rails_port": 3030,
    "uiport": 9080,
      "img_config": {
      "ext": [
        "*.png",
        "*.gif",
        "*.jpg",
        "*.jpeg",
        "*.svg"
      ]
    }
  },

  "scripts": {
    "browserSync": "node ./fe_build_scripts/browserSync.js",
    "build": "babel src -x '.js' -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "js": "asset-pipeline-browserify -x",
    "js:minify": "asset-pipeline-browserify -m -x",
    "js:watch": "asset-pipeline-browserify -w -x",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "sass:build": "node ./fe_build_scripts/sass-build.js -m",
    "sass:watch": "node ./fe_build_scripts/sass-build.js -w",
    "start": "npm run browserSync & npm run build:watch",
    "test": "npm run lint && ./node_modules/.bin/karma start fe_build_scripts/karma.conf.js --browsers PhantomJS --single-run",
    "test:watch": "karma start fe_build_scripts/karma.conf.js --browsers PhantomJS",
    "test:mac": "karma start fe_build_scripts/karma.conf.js --single-run --browsers Chrome,Firefox,Safari"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/weekly-prog.git"
  },
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/weekly-prog/issues"
  },
  "homepage": "https://github.com/djforth/weekly-prog#readme",
  "devDependencies": {
    "@djforth/ap_browserify": "^1.3.10",
    "@djforth/ap_imagemin": "2.0.3",
    "@djforth/ap_sprites": "1.0.6",
    "@djforth/ap_karma": "1.1.9",
    "@djforth/ap_utils": "^2.0.13",
    "autoprefixer": "^6.3.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.2",
    "browserify": "^13.0.0",
    "browserslist": "^1.0.1",
    "chokidar": "^1.4.3",
    "css-mqpacker": "^4.0.0",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.2.3",
    "event-stream": "^3.3.2",
    "image-size": "^0.5.0",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "node-sass-globbing": "0.0.23",
    "path": "^0.12.7",
    "postcss": "^5.0.12",
    "postcss-discard-duplicates": "^2.0.0",
    "postcss-import": "^7.1.3",
    "postcss-merge-longhand": "^2.0.1",
    "postcss-merge-rules": "^2.0.1",
    "react-addons-test-utils": "0.14.7",
    "react-jasmine": "0.1.6",
    "readdirp": "^2.0.0",
    "require-dir": "^0.3.0",
    "through2": "^2.0.1",
    "ajax-es6-module": "1.0.2",
    "datamanager": "1.0.2",
    "morse-react-mixins": "1.0.2",
    "touch-nav": "2.0.2",
    "viewport-detection-es6": "2.0.4",
    "classnames": "^2.2.0",
    "flux": "^2.1.1",
    "immutable": "^3.7.5",
    "lodash": "~4.6.1",
    "moment": "~2.12.0",
    "moment-strftime": "^0.1.5",
    "react": "0.14.8",
    "react-date-range": "0.2.4",
    "react-dom": "0.14.8"
  },
  "peerDependencies": {
    "ajax-es6-module": "1.0.2",
    "datamanager": "1.0.2",
    "morse-react-mixins": "1.0.2",
    "touch-nav": "2.0.2",
    "viewport-detection-es6": "2.0.4",
    "classnames": "^2.2.0",
    "flux": "^2.1.1",
    "immutable": "^3.7.5",
    "lodash": "^4.6.1",
    "moment": "~2.12.0",
    "moment-strftime": "^0.1.5",
    "react": "0.14.8",
    "react-date-range": "0.2.4",
    "react-dom": "0.14.8"
  },
  "dependencies": {}
}
