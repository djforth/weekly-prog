{
  "name": "@djforth/weekly-prog",
  "version": "5.3.2",
  "description": "Better Weekly Prog",
  "main": "index.js",
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ],
    "presets": [
      "es2015",
      "es2016",
      "es2017",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "morsedigital",
    "rules": {
      "react/jsx-uses-vars": 1
    }
  },
  "assets": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "images": {
      "ext": [
        "*.png",
        "*.gif",
        "*.jpg",
        "*.jpeg",
        "*.svg"
      ],
      "plugins": [
        [
          "imagemin-gifsicle",
          {
            "interlaced": true
          }
        ],
        [
          "imagemin-jpegtran",
          {
            "progressive": true
          }
        ],
        [
          "imagemin-optipng",
          {
            "optimizationLevel": 3
          }
        ],
        "imagemin-svgo"
      ]
    },
    "javascripts": {
      "ext": [
        ".js",
        ".es6.js"
      ],
      "files": [
        "application.js"
      ],
      "ignore": [],
      "externals": [],
      "separate": true,
      "input": "test_assets/javascripts",
      "output": "app/assets/javascripts",
      "common": "common.js",
      "transforms": [
        [
          "babelify",
          {
            "presets": [
              "es2015",
              "react"
            ]
          }
        ]
      ]
    },
    "karma": {
      "input": "./spec"
    },
    "port": 9090,
    "rails_port": 3030,
    "uiport": 8080
  },
  "config": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "port": 4000,
    "rails_port": 3030,
    "uiport": 9080,
    "img_config": {
      "ext": [
        "*.png",
        "*.gif",
        "*.jpg",
        "*.jpeg",
        "*.svg"
      ]
    }
  },
  "scripts": {
    "build": "babel src -x '.js' -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "js": "webpack --config webpack/webpack.config.js",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "start": "node ./webpack/server.js",
    "test": "./node_modules/karma/bin/karma start --browsers PhantomJS --single-run",
    "test:mac": "./node_modules/karma/bin/karma start --browsers Chrome,Firefox,Safari  --single-run",
    "test:watch": "node --max_old_space_size=4096 ./node_modules/karma/bin/karma start --browsers PhantomJS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/weekly-prog.git"
  },
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/weekly-prog/issues"
  },
  "homepage": "https://github.com/djforth/weekly-prog#readme",
  "devDependencies": {
    "@djforth/ap_utils": "^2.2.0",
    "@djforth/morse-jasmine-wp": "2.4.2",
    "@djforth/react-jasmine-wp": "^1.1.4",
    "ajax-es6-module": "1.0.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-rewire-ignore-coverage": "^1.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "datamanager": "1.0.2",
    "eslint": "^3.7.1",
    "eslint-config-google": "^0.6.0",
    "eslint-config-morsedigital": "^1.0.3",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flux": "^3.0.0",
    "i18n-js": "http://github.com/fnando/i18n-js/archive/v3.0.0.rc13.tar.gz",
    "immutable": "^3.8.1",
    "import-glob-loader": "^1.1.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-story-reporter": "^0.3.1",
    "karma-webpack": "^1.8.0",
    "lodash": "~4.16.3",
    "moment": "^2.15.1",
    "moment-strftime": "^0.1.5",
    "morse-react-mixins": "~1.2.1",
    "node-sass": "^3.10.1",
    "react": "~15.3.2",
    "react-addons-test-utils": "~15.3.2",
    "react-date-range": "~0.2.4",
    "react-dom": "~15.3.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "viewport-detection-es6": "2.1.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "peerDependencies": {
    "ajax-es6-module": "~1.0.*",
    "babel-polyfill": "^6.*.*",
    "datamanager": "~1.0.*",
    "morse-react-mixins": "~1.2.*",
    "viewport-detection-es6": "^2.1.*",
    "classnames": "^2.2.*",
    "flux": "^3.0.*",
    "immutable": "^3.7.*",
    "lodash": "^4.*.*",
    "moment": "^2.12.*",
    "moment-strftime": "^0.1.5",
    "react": "~15.3.*",
    "react-date-range": "~0.2.*",
    "react-dom": "~15.3.*"
  }
}
