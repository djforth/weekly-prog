{
  "name": "weekly-prog",
  "version": "1.0.30",
  "description": "Better Weekly Prog",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "config": {
    "assets_in": "test_assets",
    "assets_out": "app/assets",
    "name": "server",
    "port": 4000,
    "rails_port": 3030,
    "uiport": 9080
  },
  "img_config": {
    "ext": [
      "*.png",
      "*.gif",
      "*.jpg",
      "*.jpeg",
      "*.svg"
    ]
  },
  "js_config": {
    "ext": [
      ".js",
      ".es6.js"
    ],
    "files": [
      "application.js",
      "now.js"
    ],
    "input": "app/assets_uncompiled/javascripts",
    "output": "app/assets/javascripts",
    "common": "common.js"
  },
  "scripts": {
    "browserSync": "node ./fe_build_scripts/browserSync.js",
    "build": "babel src -x '.js' -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "javascript:build": "node ./fe_build_scripts/browserify_build.js",
    "javascript:watch": "node ./fe_build_scripts/browserify_build.js -w",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "sass:build": "node ./fe_build_scripts/sass-build.js -m",
    "sass:watch": "node ./fe_build_scripts/sass-build.js -w",
    "start": "npm run browserSync & npm run build:watch",
    "test": "npm run lint && ./node_modules/.bin/karma start fe_build_scripts/karma.conf.js --browsers PhantomJS --single-run",
    "test:watch": "karma start fe_build_scripts/karma.conf.js --browsers PhantomJS",
    "test:mac": "karma start fe_build_scripts/karma.conf.js --single-run --browsers Chrome,Firefox,Safari"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/weekly-prog.git"
  },
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/weekly-prog/issues"
  },
  "homepage": "https://github.com/djforth/weekly-prog#readme",
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-eslint": "^4.1.6",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browser-sync": "^2.10.0",
    "browserify": "^12.0.1",
    "browserslist": "^1.0.1",
    "chokidar": "^1.3.0",
    "criticalcss": "^0.6.1",
    "css-mqpacker": "^4.0.0",
    "envify": "^3.4.0",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.9.0",
    "event-stream": "^3.3.2",
    "factor-bundle": "^2.5.0",
    "image-size": "^0.3.5",
    "jasmine-core": "^2.3.4",
    "jasmine-es6-promise-matchers": "^2.0.1",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-ievms": "^0.1.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-story-reporter": "^0.3.1",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "node-sass-globbing": "0.0.23",
    "path": "^0.12.7",
    "phantomjs": "^1.9.18",
    "postcss": "^5.0.12",
    "postcss-discard-duplicates": "^2.0.0",
    "postcss-import": "^7.1.3",
    "postcss-merge-longhand": "^2.0.1",
    "postcss-merge-rules": "^2.0.1",
    "react-addons-test-utils": "^0.14.3",
    "react-jasmine": "0.1.6",
    "react-tools": "^0.13.3",
    "readdirp": "^2.0.0",
    "require-dir": "^0.3.0",
    "rewireify": "^0.2.5",
    "sinon": "^1.17.2",
    "through2": "^2.0.0",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "ajax-es6-module": "0.0.8",
    "classnames": "^2.2.0",
    "datamanager": "0.0.18",
    "@djforth/date-formatter": "1.0.3",
    "flux": "^2.1.1",
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "moment-strftime": "^0.1.5",
    "morse-react-mixins": "0.1.11",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-date-range": "0.2.2",
    "react-dom": "^0.14.3",
    "touch-nav": "1.0.16",
    "viewport-detection-es6": "0.0.9"
  }
}
